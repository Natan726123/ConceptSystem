  object FDQueryFaccao: TFDQuery
    Active = True
    Connection = Form6.FDConnection1
    SQL.Strings = (
      'select codFaccao, nomeFaccao  from TBfaccao'
      'order by nomeFaccao asc')
    Left = 136
  end
  object FDQueryConsultaFichaFaccao: TFDQuery
    Active = True
    CachedUpdates = True
    Connection = Form6.FDConnection1
    UpdateObject = UpdateSQLConsultaFicha
    SQL.Strings = (
      'SELECT dataCriacao,'
      '    nomeProduto AS Modelo,'
      '    corTecido AS Cor,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'P'#39' THEN quantidadePecas ELSE 0' +
        ' END) AS Tam_P,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'M'#39' THEN quantidadePecas ELSE 0' +
        ' END) AS Tam_M,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'G'#39' THEN quantidadePecas ELSE 0' +
        ' END) AS Tam_G,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'GG'#39' THEN quantidadePecas ELSE ' +
        '0 END) AS Tam_GG,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'48'#39' THEN quantidadePecas ELSE ' +
        '0 END) AS Tam_48,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'50'#39' THEN quantidadePecas ELSE ' +
        '0 END) AS Tam_50,'
      
        '    SUM(CASE WHEN tamanhoPecas = '#39'52'#39' THEN quantidadePecas ELSE ' +
        '0 END) AS Tam_52,'
      #9'statusOrdem AS Status,'
      '    MIN(dataCorte) AS DataCorte,'
      '    MIN(dataEnvio) AS DataEnvio,'
      '    MIN(dataPrevisao) AS DataPrevisao,'
      '    MIN(dataEntrega) AS DataEntrega,'
      '    '
      #9'idFaccao,'
      #9'numCorte,'
      #9'numOrdem,'
      #9'codCortador,'
      #9'nomeCortador'
      'FROM '
      '    TBFichaDeFaccao'
      #9
      'WHERE '
      #9'nomeProduto = :nomeProduto AND'
      #9'codFaccao = :codFaccao AND'
      #9'dataCriacao BETWEEN :dataInicial and :dataFinal'
      #9#9
      'GROUP BY '
      '    nomeProduto, corTecido, statusOrdem'
      'ORDER BY '
      '    nomeProduto, corTecido;')
    Left = 760
    Top = 8
    ParamData = <
      item
        Name = 'NOMEPRODUTO'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end
      item
        Name = 'CODFACCAO'
        DataType = ftString
        ParamType = ptInput
      end
      item
        Name = 'DATAINICIAL'
        DataType = ftString
        ParamType = ptInput
      end
      item
        Name = 'DATAFINAL'
        DataType = ftString
        ParamType = ptInput
      end>
  end
